        <!--            <item name="Trunk" href="https://maven.apache.org/doxia/doxia/index.html"/>-->
        <!--        </breadcrumbs>-->

        <menu ref="parent" inherit="top"/>
        <menu name="Overview" inherit="top">
            <item name="Foo" href="foo.html"/>
            <item name="FAQ" href="faq.html"/>
        </menu>
        <menu ref="modules" inherit="top"/>
        <menu ref="reports" inherit="top"/>

        <links>
            <item name="Apache" href="http://www.apache.org"/>
            <item name="Maven" href="https://maven.apache.org"/>
        </links>

        <footer><![CDATA[All rights reserved.]]></footer>

    </body>

    <custom>
        <fluidoSkin>
            <sideBarEnabled>false</sideBarEnabled>
            <topBarEnabled>true</topBarEnabled>
            <topBarIcon>
                <name>Maven Fluido Skin</name>
                <alt>Maven Fluido Skin</alt>
                <src>/images/topbar-logo.png</src>
                <href>/index.html</href>
            </topBarIcon>

            <navBarStyle>navbar-inverse</navBarStyle>

            <gitHub>
                <projectId>giflw/jmacro</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>

            <profile>pre-release</profile>

        </fluidoSkin>
    </custom>

</project>
